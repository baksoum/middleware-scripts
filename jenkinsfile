pipeline {
    agent any

    stages {
            stage('Hello') {
                steps {
                    echo 'Hello World This is an example of salutations'
                    sleep 3
                }
            }
            stage('QA') {
                steps {
                    echo 'QA Environnement'
                    sleep 3
                }
            }
            stage('Prod') {
                steps {
                    echo 'Prod Environnement'
                    sleep 3
                }
            }
            stage('dev') {
                steps {
                    echo 'Finaly Prod Environnement woooooooww'
                    sleep 3
                }
            }
        
    }
}
